{
  "name": "bigfootlive-react-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:prod": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "playwright test",
    "test:enterprise": "playwright test tests/e2e/13-*.spec.ts tests/e2e/14-*.spec.ts tests/e2e/15-*.spec.ts tests/e2e/16-*.spec.ts tests/e2e/17-*.spec.ts",
    "test:feature-flags": "playwright test tests/e2e/13-feature-flags.spec.ts",
    "test:events": "playwright test tests/e2e/14-events-datatable.spec.ts",
    "test:webrtc": "playwright test tests/e2e/15-webrtc-multi-presenter.spec.ts",
    "test:captions": "playwright test tests/e2e/16-closed-captioning.spec.ts",
    "test:overlays": "playwright test tests/e2e/17-overlays-lower-thirds.spec.ts",
    "test:smoke": "playwright test tests/e2e/13-*.spec.ts tests/e2e/14-*.spec.ts --workers=4",
    "test:regression": "playwright test tests/e2e/*.spec.ts",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:enterprise:prod": "playwright test --config=playwright.prod.config.ts tests/e2e/13-*.spec.ts tests/e2e/14-*.spec.ts tests/e2e/15-*.spec.ts tests/e2e/16-*.spec.ts tests/e2e/17-*.spec.ts",
    "test:prod:smoke": "playwright test --config=playwright.prod.config.ts tests/e2e/13-*.spec.ts tests/e2e/14-*.spec.ts",
    "test:prod:report": "playwright show-report test-results/html-report"
  },
  "dependencies": {
